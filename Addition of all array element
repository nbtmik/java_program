package arraypractice1a;

/**
 *
 * @author KIIT
 */
public class Arraypractice1a {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int A[]={3,9,7,8,12,6,15,5,4,10};
        int Sum=0;
        for(int i=0;i<A.length;i++){
            Sum=Sum+A[i];        
        }
        System.out.println(Sum);
    }
    
}
