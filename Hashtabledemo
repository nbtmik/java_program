package hashtabledemo;
import java.util.*;

public class Hashtabledemo {

    public static void main(String[] args) {
        Hashtable<Integer,String> ht = new Hashtable<>();
        ht.put(1, "A");
        ht.put(2, "B");
        ht.put(3, "C");
        ht.put(4, "D");
        ht.put(5, "F");
        
        ht.compute(2, (k,v)->"Z");
        ht.computeIfAbsent(7, (k)->"Z");
        
        System.out.println(ht);
        
    }
    
}
