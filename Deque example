package dequedemo;

import java.util.*;

public class DequeDemo {

    public static void main(String[] args) {
        ArrayDeque<Integer> dq = new ArrayDeque<>();
        dq.offerLast(10);
        dq.offerLast(20);
        dq.offerLast(30);
        dq.offerFirst(80);
        dq.offerFirst(90);
        dq.offerFirst(70);
        dq.pollFirst();
        System.out.println(dq);
    }
    
}
