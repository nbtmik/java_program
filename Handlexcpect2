package handlexcpect2;

/**
 *
 * @author KIIT
 */
public class Handlexcpect2 {

    public static void main(String[] args) {
        int A[]={1,2,0,3,4};
        try{
            int r= A[4]/A[1];
            System.out.println(r);
            System.out.println(A[10]);
        }
        catch(ArithmeticException e){
            System.out.println(e);
        }
        catch(Exception e){
            System.out.println(e);
        }
    }
    
}
