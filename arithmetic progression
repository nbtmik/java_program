package ap;

import java.util.Scanner;

/**
 *
 * @author KIIT
 */
public class AP {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int n , d , a;
        Scanner s= new Scanner(System.in);
        System.out.println("Enter a,d and n");
        a=s.nextInt();  
        d=s.nextInt();
        n=s.nextInt();
        int term = a;
        for(int i=0;i<n;i++){
            System.out.print(term+",");
            term = term+d;
        }
    }
    
}
