package generic;

class MyArray<T>{
    T A[]=(T[])new Object[10];
    int length = 0;
    public void append(T v){
        A[length++]=v;
    }
    public void display(){
        for(T x:A){
            System.out.println(x);
        }
    }
}
public class Generic {

    public static void main(String[] args) {
        MyArray<Integer> ma = new MyArray<>();
        ma.append(10);
        ma.append(20);
        ma.append(30);
        ma.display();
    }
    
}
