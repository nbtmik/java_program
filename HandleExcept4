public class HandleExcept4 {

   static void fun1(){
     int a=10,b=0,c;
             try{
                 c=a/b;
                System.out.println(c);
             }
             catch(Exception e){
                System.out.println(e.getMessage());
             }
   }
    
    public static void main(String[] args) {
        fun1();
    }
    
}
